@model CalculatorCode.Models.PermutationsCalculator

@{
    ViewData["Title"] = "Permutations Calculator";

    String algorithmCode = ViewBag.Code;
}


<body>

    @using (Html.BeginForm())
    {
        @Html.DisplayNameFor(model => model.NumberOfObjects)
        <span> <br /> </span>
        @Html.EditorFor(model => model.NumberOfObjects)
        <span> <br /> </span>
        @Html.DisplayNameFor(model => model.SampleSize)
        <span> <br /> </span>
        @Html.EditorFor(model => model.SampleSize)
        <br />
        <input type="submit" class="btn btn-success" />
        <span> <br /> </span>
        <h4>Result (Permutations): </h4>
        @Html.DisplayFor(model => model.Result)

    }
</body>

