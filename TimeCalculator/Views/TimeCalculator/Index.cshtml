@model TimeCalculator.Models.TimeCalculatorModel

@{
    ViewData["Title"] = "Home Page";
}


<body>


    <h2> Date-time calculator</h2>
    @using (Html.BeginForm())
    {
        @Html.EditorFor(model => model.InputOne)
        <span> <br /> </span>
        @Html.EditorFor(model => model.InputTwo)
        <input type="submit" class="btn btn-success" />
        <span> <br /> </span>
        <h4>Result: @Html.DisplayFor(model => model.Result) hrs</h4>
    
    }

</body>
